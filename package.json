{
  "name": "k8s-form-in-action",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "rm -rf demo && rm -rf dist && yarn build:lib && yarn build:demo && cp -r dist/k8s-form-in-action demo",
    "build:demo": "ng build --prod --base-href /k8s-form-in-action/demo/ --deploy-url /k8s-form-in-action/demo/",
    "build:lib": "ng-packagr -p projects/ng-resource-form-util",
    "debug": "ng-packagr -p projects/ng-resource-form-util/ng-package.debug.json",
    "prerelease": "yarn test && yarn build",
    "release": "npm publish dist/ng-resource-form-util",
    "test": "ng test",
    "test:watch": "WATCH=true ng test",
    "lint": "ng lint"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^11.0.4",
    "@angular/common": "^11.0.4",
    "@angular/compiler": "^11.0.4",
    "@angular/core": "^11.0.4",
    "@angular/forms": "^11.0.4",
    "@angular/platform-browser": "^11.0.4",
    "@angular/platform-browser-dynamic": "^11.0.4",
    "@angular/router": "^11.0.4",
    "core-js": "^3.8.1",
    "js-yaml": "^3.14.1",
    "lodash-es": "^4.17.15",
    "markdown-it": "^12.0.3",
    "monaco-editor": "^0.20.0",
    "monaco-yaml": "~2.4.1",
    "ng-monaco-editor": "^2.3.0",
    "rxjs": "^6.5.2",
    "tslib": "^2.0.0",
    "yaml-js": "^0.2.3",
    "zone.js": "^0.11.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1100.4",
    "@angular/cli": "^11.0.4",
    "@angular/compiler-cli": "^11.0.4",
    "@angular/language-service": "^11.0.4",
    "@types/jasmine": "^3.3.13",
    "@types/jasminewd2": "^2.0.3",
    "@types/js-yaml": "^3.12.5",
    "@types/lodash-es": "^4.17.3",
    "@types/markdown-it": "^10.0.3",
    "@types/node": "^14.14.12",
    "@types/webpack-env": "^1.16.0",
    "codelyzer": "^6.0.1",
    "css-element-queries": "^1.2.3",
    "husky": "^4.3.5",
    "jasmine-core": "^3.4.0",
    "jasmine-spec-reporter": "^6.0.0",
    "karma": "^5.1.1",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage-istanbul-reporter": "^3.0.3",
    "karma-jasmine": "^4.0.1",
    "karma-jasmine-html-reporter": "^1.5.4",
    "lint-staged": "^10.5.3",
    "ng-packagr": "^11.0.0",
    "prettier": "^2.2.1",
    "ts-node": "^9.1.1",
    "tslint": "^6.1.3",
    "typescript": "~4.0.5"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all",
    "semi": true
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
