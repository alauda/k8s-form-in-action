{
  "name": "k8s-form-in-action",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "run-p build:*",
    "build:demo": "ng build --prod",
    "build:lib": "ng-packagr -c . -p projects/ng-resource-form-util",
    "debug": "ng-packagr -p projects/ng-resource-form-util/ng-package.debug.json",
    "lint": "run-p lint:*",
    "lint:es": "eslint . --cache --ext md,js,ts --max-warnings 10",
    "lint:style": "stylelint . --cache",
    "lint:ts": "tslint -p . -t stylish",
    "lint:tsc": "tsc --noEmit",
    "release": "cd projects/ng-resource-form-util && standard-version --no-verify --release-as",
    "start": "ng serve --hmr",
    "test": "ng test",
    "test:watch": "WATCH=true ng test"
  },
  "devDependencies": {
    "@1stg/app-config": "^1.2.5",
    "@1stg/tslint-config": "^1.0.0",
    "@angular-devkit/build-angular": "^0.1102.2",
    "@angular/animations": "^11.2.3",
    "@angular/cli": "^11.2.2",
    "@angular/common": "^11.2.3",
    "@angular/compiler": "^11.2.3",
    "@angular/compiler-cli": "^11.2.3",
    "@angular/core": "^11.2.3",
    "@angular/forms": "^11.2.3",
    "@angular/language-service": "^11.2.3",
    "@angular/platform-browser": "^11.2.3",
    "@angular/platform-browser-dynamic": "^11.2.3",
    "@angular/router": "^11.2.3",
    "@types/jasmine": "^3.6.4",
    "@types/jasminewd2": "^2.0.3",
    "@types/js-yaml": "^4.0.0",
    "@types/lodash-es": "^4.17.4",
    "@types/markdown-it": "^12.0.1",
    "@types/node": "^14.14.31",
    "@types/webpack-env": "^1.16.0",
    "core-js": "^3.9.0",
    "jasmine-core": "^3.4.0",
    "jasmine-spec-reporter": "^6.0.0",
    "jest": "^26.6.3",
    "js-yaml": "^4.0.0",
    "karma": "^6.1.1",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage-istanbul-reporter": "^3.0.3",
    "karma-jasmine": "^4.0.1",
    "karma-jasmine-html-reporter": "^1.5.4",
    "lodash-es": "^4.17.21",
    "markdown-it": "^12.0.4",
    "monaco-editor": "^0.22.3",
    "monaco-yaml": "~2.4.1",
    "ng-monaco-editor": "^3.0.0",
    "ng-packagr": "^11.2.4",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.6",
    "standard-version": "^9.1.1",
    "ts-node": "^9.1.1",
    "tslib": "^2.1.0",
    "tslint": "^6.1.3",
    "typescript": "^4.2.2",
    "yaml-js": "^0.2.3",
    "zone.js": "^0.11.4"
  },
  "resolutions": {
    "prettier": "^2.2.1"
  }
}
